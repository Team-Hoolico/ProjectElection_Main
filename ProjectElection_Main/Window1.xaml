                        </Ellipse>
                        <ContentPresenter HorizontalAlignment="Center"  
                                    VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Width="100" Content=""  
            Height="100" RenderTransformOrigin="0.516,0.522" Margin="120,26,1700,954">
