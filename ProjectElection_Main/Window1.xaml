            </Button.Template>
        </Button>
        <TextBlock HorizontalAlignment="Left" Margin="588,126,0,0" TextWrapping="Wrap" Text="DASH" VerticalAlignment="Top" RenderTransformOrigin="0.471,0.729" Foreground="White" FontSize="36"/>
        <TextBlock HorizontalAlignment="Left" Margin="613,170,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" RenderTransformOrigin="0.471,0.729" Foreground="White" FontSize="10"/>
        <TextBlock HorizontalAlignment="Left" Margin="95,126,0,0" TextWrapping="Wrap" Text="Unlocked" VerticalAlignment="Top" RenderTransformOrigin="0.471,0.729" Foreground="White" FontSize="36"/>
        <TextBlock HorizontalAlignment="Left" Margin="149,170,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" RenderTransformOrigin="0.471,0.729" Foreground="White" FontSize="10"/>
        <Image HorizontalAlignment="Left" Height="137" Margin="340,88,0,0" VerticalAlignment="Top" Width="125" Source="/tedicon.jpg">
            <Image.OpacityMask>
                <ImageBrush ImageSource="/tedicon.jpg"/>
