        </Rectangle>
        <Rectangle HorizontalAlignment="Left" Height="348" Margin="242,41,0,0" VerticalAlignment="Top" Width="438" RenderTransformOrigin="0.5,0.5">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
