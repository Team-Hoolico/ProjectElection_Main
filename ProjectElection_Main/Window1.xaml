        <RadioButton Content="Josie İlkim Çakmak" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" FontFamily="Bahnschrift Light" Background="White" Foreground="White" Margin="74,297,0,0"/>
        <RadioButton Content="Nesibe Özel" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" FontFamily="Bahnschrift Light" Background="White" Foreground="White" Margin="74,251,0,0"/>
        <RadioButton Content="Nisan Üstün" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" FontFamily="Bahnschrift Light" Background="White" Foreground="White" Margin="74,205,0,0"/>
